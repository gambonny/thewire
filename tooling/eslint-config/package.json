{
  "name": "@monorepo/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "main": "eslint.config.js",
  "types": "index.d.ts",
  "exports": {
    "./base": "./eslint.config.js",
    "./react": "./react.config.js",
    ".": "./eslint.config.js"
  },
  "scripts": {
    "format": "wireit"
  },
  "engines": {
    "node": "22.12.0",
    "pnpm": "10.6.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "@eslint/js": "^9.22.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@vitest/eslint-plugin": "^1.1.37",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "^8.26.1"
  },
  "wireit": {
    "format": {
      "command": "prettier --ignore-path ../../.prettierignore --write .",
      "files": [
        "eslint.config.js"
      ]
    }
  }
}
